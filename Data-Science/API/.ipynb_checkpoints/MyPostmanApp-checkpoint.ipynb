   "source": [
    "def findURL():\n",
    "    import requests\n",
    "    baseUrl = input(\"Please enter base url: \")\n",
    "    endpoint = input(\"Please enter endpoint: \")\n",
    "    query = input(\"Please enter query: \")\n",
    "    URL = requests.get(baseUrl + endpoint + query)\n",
    "    info=URL.json()\n",
    "    return info\n"
   ]
